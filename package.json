{
  "name": "redux-form-uikit",
  "version": "3.0.0-beta.1",
  "description": "A set of wrapper components to facilitate using UIkit React with Redux Form",
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "coveralls": "coveralls < .coverage/lcov.info",
    "prebuild": "rm -rf lib",
    "build": "babel ./src --ignore __tests__,stories --out-dir ./lib",
    "prepublish": "yarn run build",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook -o .out -s .storybook/static",
    "deploy-storybook": "storybook-to-ghpages",
    "postpublish": "gh-release && yarn run deploy-storybook",
    "postversion": "git add package.json && git commit -m \"chore(package): update version\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stipsan/redux-form-uikit.git"
  },
  "keywords": [
    "react",
    "react-component",
    "redux",
    "redux-form",
    "uikit",
    "uikit-react",
    "ecosystem-react"
  ],
  "author": "Stian Didriksen <stipsan@gmail.com> (http://github.com/stipsan)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stipsan/redux-form-uikit/issues"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "coverageDirectory": "<rootDir>/.coverage",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ]
  },
  "homepage": "https://github.com/stipsan/redux-form-uikit",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.4.0",
    "@kadira/storybook": "2.35.3",
    "@kadira/storybook-deployer": "1.2.0",
    "autoprefixer": "7.0.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "chokidar": "1.7.0",
    "coveralls": "2.13.1",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.0",
    "expect": "1.20.2",
    "file-loader": "0.11.1",
    "gh-release": "2.2.1",
    "growl": "1.9.2",
    "jest-cli": "20.0.1",
    "jsdom": "10.1.0",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "postcss-loader": "2.0.5",
    "prop-types": "15.5.10",
    "raw-loader": "0.5.1",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-test-renderer": "15.5.4",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "semver": "5.3.0",
    "semver-truncate": "1.1.2",
    "style-loader": "0.17.0",
    "uikit": "3.0.0-beta.22",
    "uikit-react": "3.0.0-beta.2",
    "url-loader": "0.5.8"
  },
  "dependencies": {
    "classnames": "2.2.5"
  }
}
